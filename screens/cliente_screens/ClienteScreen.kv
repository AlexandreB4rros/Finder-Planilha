# Azul


<ListaBipagem@MDGridLayout>
    cols: 2

<ClienteScreen>
    name: 'ClienteScreen'

    BoxLayout:
        orientation: "vertical"
        MDTopAppBar:
            title: "Finder Planilha"
            elevation: 10
            anchor_title: 'left'
            #md_bg_color: 0.90, 0.05, 0.06, 1
            right_action_items: [["information-variant-circle-outline", lambda x: root.aplicar_config(), "aplicar configuração"]]

        MDBottomNavigation:
            #panel_color: 0.90, 0.05, 0.06, 1

            MDBottomNavigationItem:
                name: 'arquivos'
                icon: 'file-document-outline'
                text: 'Planilhas'

            MDBottomNavigationItem:
                name: 'documentar'
                icon: 'file-document-plus-outline'
                text: 'Nova Planilha'

                MDCard:
                    elevation: 1
                    size_hint: 0.889, .25
                    pos_hint: {"center_x": 0.5, "center_y": 0.80}
                    Image:
                        source: 'img/icon.png'

                MDTextField:
                    id: Nome_Cidade

                    hint_text: 'CIDADE:'
                    mode: "rectangle"
                    size_hint_x: 0.889
                    pos_hint: {"center_x": 0.5, "center_y": 0.55}

                MDTextField:
                    id: Nome_Pop

                    hint_text: 'POP:'
                    mode: "rectangle"
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.25, "center_y": 0.45}

                MDTextField:
                    id: Nome_Olt

                    hint_text: 'OLT:'
                    text: "1"
                    mode: "rectangle"
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.75, "center_y": 0.45}

                MDTextField:
                    id: Nome_Slot

                    hint_text: 'SLOT:'
                    mode: "rectangle"
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.25, "center_y": 0.35}

                MDTextField:
                    id: Nome_Pon

                    hint_text: 'PON:'
                    mode: "rectangle"
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.75, "center_y": 0.35}

                MDRaisedButton:
                    text: "Limpar as informação"
                    elevation: 3
                    #md_bg_color: 0.90, 0.05, 0.06, 1
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.25, "center_y": 0.175}
                    on_release:
                        root.Limpar_Widget()

                MDRaisedButton:
                    text: "Informação Fake"
                    elevation: 3
                    #md_bg_color: 0.90, 0.05, 0.06, 1
                    size_hint_x: 0.40
                    pos_hint: {"center_x": 0.75, "center_y": 0.175}
                    on_release:
                        root.Info_Fake()

                MDRaisedButton:
                    text: "Próximo"
                    #md_bg_color: 0.90, 0.05, 0.06, 1
                    pos_hint: {'center_x': 0.5, 'center_y': 0.110}
                    size_hint_x: 0.889
                    elevation: 3
                    on_release:
                        root.proxima_tela_cto()

            MDBottomNavigationItem:
                name: 'user'
                icon: 'account-circle-outline'
                text: 'Usuário'

                MDGridLayout:
                    cols: 2
                    size_hint_x: 0.70
                    pos_hint: {"center_x": 0.5, "center_y": 0.40}
                    ListaBipagem:
                        MDCheckbox:
                            id: selecionar_tema_escuro
                            group: 'selecionar_tema'
                        MDLabel:
                            text: "Dark"
                    ListaBipagem:
                        MDCheckbox:
                            id: selecionar_tema_claro
                            group: 'selecionar_tema'
                        MDLabel:
                            text: "Ligth"

                Image:
                    size_hint: .3, .3
                    pos_hint: {"center_x": 0.5, "center_y": 0.80}
                    source: 'img/user.png'

                MDLabel:
                    text: "Alexandre B."
                    font_style: "H5"
                    role: "small"
                    #size_hint_x: 0.5
                    pos_hint: {"center_x": .8, "center_y": .65}

                MDTextField:
                    id: Nome_User

                    hint_text: 'User:'
                    #mode: "rectangle"
                    size_hint_x: 0.75
                    pos_hint: {"center_x": 0.5, "center_y": 0.50}

                MDRaisedButton:
                    text: "Salvar"
                    #md_bg_color: 0.90, 0.05, 0.06, 1
                    pos_hint: {'center_x': 0.5, 'center_y': 0.110}
                    size_hint_x: 0.75
                    elevation: 3
                    on_release:
                        root.aplicar_config()

#********************************** TELA 02 - INDENTIFICAÇÃO DA CTO **********************************

<Selecionar_spliter>
    name: 'Selecionar_spliter'

    cto_field: cto
    obs_field: obs

    BoxLayout:
        orientation: "vertical"
        MDTopAppBar:
            #md_bg_color: 0.90, 0.05, 0.06, 1
            title: "Finder Planilha"
            elevation: 2
            anchor_title: 'left'
            #pos_hint: {"center_x": 0.5, "center_y": 0.95}
            left_action_items: [["chevron-left", lambda x: root.tela_anterior(), "Tela Principal"]]
            right_action_items: [["information-variant-circle-outline", lambda x: root.aplicar_config(), "aplicar configuração"]]

        MDFloatLayout:
            MDGridLayout:
                cols: 2
                pos_hint: {"center_x": 0.5, "center_y": 0.50}

                ListaBipagem:
                    MDCheckbox:
                        id: umpraoito
                        group: 'indentidade_cto'
                    MDLabel:
                        text: "8 - Portas"
                ListaBipagem:
                    MDCheckbox:
                        id: umpradezeseis
                        group: 'indentidade_cto'
                    MDLabel:
                        text: "16 - Portas"

            MDCard:
                elevation: 1
                size_hint: 0.889, .25
                pos_hint: {"center_x": 0.5, "center_y": 0.80}
                Image:
                    source: 'img/icon.png'

            MDTextField:
                id: cto

                hint_text: 'Identifique a cto:'
                mode: "rectangle"
                size_hint_x: 0.889
                pos_hint: {"center_x": 0.5, "center_y": 0.60}

            MDTextField:
                id: obs

                hint_text: 'Observação:'
                mode: "rectangle"
                multiline: True
                size_hint: 0.889, .20
                pos_hint: {"center_x": 0.5, "center_y": 0.35}

            MDRaisedButton:
                text: "Limpar as informação"
                elevation: 3
                #md_bg_color: 0.90, 0.05, 0.06, 1
                size_hint_x: 0.40
                pos_hint: {"center_x": 0.25, "center_y": 0.175}
                on_release:
                    root.Limpar_Widget()

            MDRaisedButton:
                text: "Informação Fake"
                elevation: 3
                #md_bg_color: 0.90, 0.05, 0.06, 1
                size_hint_x: 0.40
                pos_hint: {"center_x": 0.75, "center_y": 0.175}
                on_release:
                    root.Info_Fake()

            MDRaisedButton:
                text: "Próximo"
                #md_bg_color: 0.90, 0.05, 0.06, 1
                pos_hint: {'center_x': 0.5, 'center_y': 0.110}
                size_hint_x: 0.889
                elevation: 3
                on_release:
                    root.proxima_tela()



