<tela1x8>
    name: 'tela1x8'

    MDBoxLayout:
        orientation: "vertical"
        MDTopAppBar:
            #md_bg_color: 0.90,0.05,0.06,1
            title: "Finder Planilha"
            elevation: 2.5
            anchor_title: 'left'
            pos_hint_x: {'top': 1}
            left_action_items: [["arrow-left-bold", lambda x: root.tela_anterior()]]
            right_action_items: [["broom", lambda x: root.fachina()], ["file-send-outline", lambda x: root.salvar_cto()]]

        MDBoxLayout:
            orientation: "vertical"
            MDScrollView:
                MDList:
                    id: lista

                    TwoLineRightIconListItem:
                        text: "Porta - 1"
                        secondary_text: root.time_text01
                        on_release:
                            root.manager.current = "PA01"
                            #root.update_secondary_text()
                        IconRightWidget:
                            icon: "clock"
                            on_release:
